<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="enableAsync" format="boolean"/>
    <attr name="isSelected" format="boolean" />

    <declare-styleable name="ToggleImageButton">
        <attr name="enableAsync"/>
        <attr name="isSelected" />
        <attr name="normalDrawableRes" format="reference" />
        <attr name="selectedDrawableRes" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SelectButton">
        <attr name="isSelected" />
        <attr name="normalText" format="string" />
        <attr name="normalTextColor" format="reference" />
        <attr name="selectedText" format="string" />
        <attr name="selectedTextColor" format="reference" />
        <attr name="normalTextSize" format="dimension" />
        <attr name="selectedTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ToggleButton">
        <attr name="enableAsync" />
    </declare-styleable>

    <declare-styleable name="SlideViewPager">
        <attr name="android:orientation"/>
    </declare-styleable>

    <declare-styleable name="ClickScaleButton">
        <attr name="scaleXTo" format="float" />
        <attr name="scaleYTo" format="float" />
        <attr name="duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="CURightClickVisibleContentEditText">
        <attr name="showImage" format="reference"/>
        <attr name="hideImage" format="integer" />
    </declare-styleable>

</resources>